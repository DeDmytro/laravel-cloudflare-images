{
    "name": "dedmytro/laravel-cloudflare-images",
    "description": "Cloudflare API client and filesystem for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "cloudflare",
        "cloudflare api",
        "cloudflare images"
    ],
    "authors": [
        {
            "name": "Hodun Dmytro",
            "email": "godundmytro@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/DeDmytro/laravel-cloudflare-images/issues",
        "source": "https://github.com/DeDmytro/laravel-cloudflare-images"
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "guzzlehttp/guzzle": "^6.2.1|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.5"
    },
    "autoload": {
        "psr-4": {
            "DeDmytro\\CloudflareImages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "DeDmytro\\CloudflareImages\\CloudflareImagesServiceProvider"
            ],
            "aliases": {
                "CloudflareImages": "DeDmytro\\CloudflareImages\\Facades\\CloudflareApi"
            }
        }
    }
}
